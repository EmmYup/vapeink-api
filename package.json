    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "sails-generate-eslintrc": "https://github.com/EmmanuelYupit/sails-generate-eslintrc",
    "sails-memory": "^0.10.7",
    "sails-mongo": "^0.12.2"
