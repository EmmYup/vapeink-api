{
  "name": "vapeink-api",
  "private": true,
  "version": "0.0.1",
  "description": "VapeInk api",
  "keywords": [],
  "dependencies": {
    "include-all": "^1.0.0",
    "rc": "1.0.1",
    "sails": "~0.12.13",
    "sails-disk": "~0.10.9"
  },
  "scripts": {
    "precommit": "lint-staged",
